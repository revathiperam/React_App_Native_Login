{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/App.tsx",
    "app/(tabs)/forgotpassword.tsx",
    "app/(tabs)/login.tsx",
    "app/(tabs)/signup.tsx"
  ]
}